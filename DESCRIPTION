Package: mldash
Title: Evaluating Machine Learning Models Across Many Datasets
Version: 0.1.0
Authors@R: c(
    person("Jason", "Bryer", , "jason@bryer.org", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0002-2454-0402")),
